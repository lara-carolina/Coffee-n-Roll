<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.ContTelaPrincipal">

    <MenuBar prefHeight="30.0" prefWidth="800.0" style="-fx-background-color: #e8ddc5;"
             AnchorPane.topAnchor="0.0" AnchorPane.leftAnchor="0.0">
        <Menu text="Home"/>
        <Menu text="Consulta"/>
    </MenuBar>

    <ImageView AnchorPane.topAnchor="30.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.bottomAnchor="0.0" preserveRatio="true" fitWidth="800">
        <image>
            <Image url="@/imgs/Coffee.png"/>
        </image>
    </ImageView>

</AnchorPane>
